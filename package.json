{
  "name": "@eduvault/sdk-js",
  "version": "0.2.5",
  "private": false,
  "access": "public",
  "description": "Quickly create and authenticate a user-owned database",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/wrapper.js"
},
  "typings": "dist/cjs/index.d.ts",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "repository": "https://github.com/eduvault/sdk-js",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "clean:dist": "rimraf dist/cjs",
    "clean:modules": "rimraf node_modules package-lock.json",
    "inst": "npm install",
    "prepublishOnly": "run-s build",
    "prebuild": "npm run clean:dist",
    "build": "run-p build:*",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "watch:cjs": "tsc -p tsconfig.cjs.json -w",
    "dev": "run-p watch:cjs",
    "dev:watch": "TS_NODE_PROJECT='./tsconfig.cjs.json' ts-node-dev --inspect --poll --respawn ./src src/index.ts",
    "test": "jest --env=./jest-env.js --detectOpenHandles",
    "test:watch": "jest --env=./jest-env.js --detectOpenHandles --watch",
    "test:clear-cache": "jest --clearCache"
  },

  "dependencies": {
    "@textile/hub": "^6.3.2",
    "@textile/threaddb": "^0.2.2",
    "@types/lodash": "^4.14.178",
    "axios": "^0.24.0",
    "buffer": "^6.0.3",
    "core-js": "^3.20.1",
    "core-js-bundle": "^3.20.1",
    "crypto-js": "^4.1.1",
    "google-protobuf": "^3.19.1",
    "lodash": "^4.17.21",
    "node-fetch": "^3.1.0",
    "sqlite3": "^5.0.2",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "cspell": "^5.14.0",
    "domino": "^2.1.6",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.3",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^27.4.5",
    "msw": "^0.36.3",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "files": [
    "dist/cjs",
    "dist/esm",
    "!**/*.spec.*",
    "!**/*.json",
    "LICENSE",
    "README.md"
  ],
  "prettier": {
    "singleQuote": true
  }
}
